<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <a href="/subjects/add">add subjects</a>
    <table border>
      <tr>
        <th>no</th>
        <th>id</th>
        <th>subject_name</th>
        <th colspan="2">action</th>
      </tr>
      <%let i = 1%>
      <%rows.forEach(row=>{%>
        <tr>
          <td><%=i++%></td>
          <td><%=row.id%></td>
          <td><%=row.subject_name%></td>
          <td> <a href="/subjects/edit/<%=row.id%>">edit</a> </td>
          <td> <a href="/subjects/delete/<%=row.id%>">delete</a> </td>
        </tr>
        <%})%>
    </table>
  </body>
</html>
