<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h3>welcome <%=rows.username%></h3>
    <a href="/users/<%=rows.id%>/addsubject">ambil subject</a>

    <table border>
      <tr>
        <th>no</th>
        <th>Subject</th>
        <th>Score</th>
        <th>Subject test</th>
        <th>action</th>
      </tr>
      <%let i=1%>
      <%rows.Subjects.forEach(row=>{%>
        <tr>
          <td><%=i++%></td>
          <td><%=row.subject_name%></td>
          <td><%=row.UserSubject.score%></td>
          <td><a href="/users/<%=row.id%>/test">ambil test</td>
          <td><a href="/users/subjectdelete/<%=row.id%>">delete</td>
        </tr>
        <%})%>
    </table>
  </body>
</html>
