<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h3><%=rowSubs.subject_name%> test</h3>
    <form class="" action="" method="post">
    <table border>
      <tr>
        <th>no</th>
        <th>id</th>
        <th>question</th>
        <th colspan="2">answer</th>
      </tr>
      <%let i = 1%>

      <%rowQuest.forEach(row=>{%>
        <tr>
          <td><%= i++ %></td>
          <td><%= row.id %></td>
          <td><%= row.question %></td>
          <td>
            <select class="" name="answer">
              <option value="">>SELECT<</option>
              <option value="true">True</option>
              <option value="false">False</option>
            </select>
           </td>
        </tr>
        <%})%>

    </table>
      <button type="submit" name="button">SUBMIT</button>
      </form>
  </body>
</html>
