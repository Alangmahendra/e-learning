<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h3><%=rowSubs.subject_name%> test</h3>
    <table border>
      <tr>
        <th>no</th>
        <th>question</th>
        <th colspan="2">answer</th>
      </tr>
      <%let i = 1%>
      <%rowQuest.forEach(row=>{%>
        <tr>
          <td><%= i++ %></td>
          <td><%= row.question %></td>
          <td><%= row.answer %></td>
        </tr>
        <%})%>
    </table>
  </body>
</html>
